---
layout: base
schema: blog
pagination:
    data: runs
    size: 1
    alias: run
permalink: "/runs/{{ run.id | url }}/{{ run.title | slugify }}/"
preloads:
    href: '/assets/fonts/robotomono/robotomono-variablefont_wght-webfont.woff2'
    as: 'font'
    type: 'font/woff2'
    crossorigin: true
---


<article class="region blog">
    <div class="wrapper flow prose">
        <h1 class="gradient-text">{{ run.title }}</h1>

        {% set definedDate = run.date_published %} {% include "partials/date.njk" %}

        {{ run.content_html | safe }}
    </div>
</article>

